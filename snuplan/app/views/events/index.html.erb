


<%= form_with( url: events_index_path, method: "post", class: "main_tags_form", class: "hahaha") do |f| %>			
				<% @tags.each do |t| %>
				<ul>
					<!-- 태그의 id를 params로 전달-->
					<li class = "tag_list"><%= f.check_box :search, {:id => "#{t}", :class =>"main_tag",multiple: true}, "#{t.id}", nil %>
						<%= f.label :search, "#{t.name}", :for => "#{t}", :class =>"tag_label" %></li>
					</ul>
					<% end %>


<!-- search_field 검색어 안넣었을때 nil값 출력하도록 해야한다. 젬쓰면서 보자!!-->
	<div id="index-search">
		<%= search_field(:user, :name, :class=>"search") %>
		<%= submit_tag("행사찾기", :class=>"search-button")%>
		<% end %>

	</div>
<div id = "before_search">
<% @s.each do |a|%>
<%= a.title %>
<%= link_to '자세히 보기', events_show_path(:id => a.id), remote: true, method: "post"%>
<% end %>
</div>

<div id = "search_results">
</div>

<div id = "show">

</div>

